FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
para {#include        "../Parameters"}

phases          (water air);

water
{
    transportModel  Newtonian;
    nu              $para/nu_water;
    rho             $para/rho_water;
}

air
{
    transportModel  Newtonian;
    nu              $para/nu_air;
    rho             $para/rho_air;
}

sigma           0;


